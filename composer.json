{
    "name": "sulu/article-bundle",
    "description": "Sulu bundle for managing articles",
    "type": "sulu-bundle",
    "license": "MIT",
    "require": {
        "php": "^5.5 || ^7.0",
        "sulu/sulu": "dev-develop as 1.5.0",
        "ongr/elasticsearch-bundle": "^1.2.9 || ~5.0",
        "ongr/elasticsearch-dsl": "^2.2.2 || ~5.0",
        "jackalope/jackalope": "^1.2.8 || >=1.3.3"
    },
    "require-dev": {
        "zendframework/zend-stdlib": "2.3.1 as 2.0.0rc5",
        "zendframework/zendsearch": "2.*@dev",
        "symfony/monolog-bundle": "2.4.*",
        "doctrine/doctrine-bundle": "1.4.*",
        "jackalope/jackalope-doctrine-dbal": "^1.2.5",
        "jackalope/jackalope-jackrabbit": "^1.2",
        "phpunit/phpunit": "^4.8 || ^5.0",

        "sulu/document-manager": "dev-develop as 0.9.0",
        "massive/search-bundle": "@dev",

        "sulu/automation-bundle": "^1.0",
        "php-task/task-bundle": "dev-master as 1.1.0",
        "php-task/php-task": "dev-master as 1.1.0"
    },
    "suggest": {
        "sulu/automation-bundle": "Allows to outsource long-running route update processes."
    },
    "keywords": [
        "articles"
    ],
    "authors": [
        {
            "name": "Sulu Article",
            "homepage": "https://github.com/sulu/SuluArticleBundle/contributors"
        }
    ],
    "homepage": "https://github.com/sulu/SuluArticleBundle",
    "autoload": {
        "psr-4": {
            "Sulu\\Bundle\\ArticleBundle\\": ""
        }
    }
}
