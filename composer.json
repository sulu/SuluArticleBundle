{
    "name": "sulu/article-bundle",
    "description": "Sulu bundle for managing articles",
    "type": "sulu-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "sulu/sulu": "^2.0",
        "symfony/config": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/force-lowest": "^3.4",
        "symfony/http-foundation": "^3.4",
        "symfony/http-kernel": "^3.4",
        "symfony-cmf/slugifier-api": "^1.0 || ^2.0",
        "ongr/elasticsearch-bundle": "^1.2.10 || ^5.2",
        "ongr/elasticsearch-dsl": "^2.2.2 || ~5.0",
        "jackalope/jackalope": "^1.2.8 || >=1.3.3",
        "jms/serializer-bundle": "^1.1 || ^2.0"
    },
    "require-dev": {
        "massive/build-bundle": "~0.3.0",
        "zendframework/zend-stdlib": "~2.3",
        "zendframework/zendsearch": "@dev",
        "symfony/browser-kit": "^3.4",
        "symfony/monolog-bundle": "^3.1",
        "symfony/stopwatch": "^3.4",
        "jackalope/jackalope-doctrine-dbal": "^1.2.5",
        "jackalope/jackalope-jackrabbit": "^1.2",
        "phpunit/phpunit": "^4.8 || ^5.0",
        "sulu/automation-bundle": "^2.0"
    },
    "conflict": {
        "php": "^7.2",
        "ongr/elasticsearch-bundle": "^5.0.6",
        "sulu/automation-bundle": "^1.2",
        "php-task/task-bundle": "^1.2",
        "php-task/php-task": "^1.2",
        "doctrine/data-fixtures": "^1.0"
    },
    "suggest": {
        "sulu/automation-bundle": "Allows to outsource long-running route update processes."
    },
    "keywords": [
        "articles"
    ],
    "authors": [
        {
            "name": "Sulu Article",
            "homepage": "https://github.com/sulu/SuluArticleBundle/contributors"
        }
    ],
    "homepage": "https://github.com/sulu/SuluArticleBundle",
    "autoload": {
        "psr-4": {
            "Sulu\\Bundle\\ArticleBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-sulu-2.0": "2.0-dev"
        }
    }
}
