<?xml version="1.0" ?>
<list xmlns="http://schemas.sulu.io/list-builder/list">
    <key>articles</key>

    <properties>
        <property
            name="id"
            visibility="no"
            sortable="false"
            translation="sulu_admin.id"
        />
        <property
            name="typeTranslation"
            visibility="no"
            translation="sulu_admin.type"
        />
        <property
            name="title"
            visibility="always"
            translation="sulu_admin.title"
        />
        <property
            name="creatorFullName"
            visibility="no"
            translation="sulu_admin.creator"
        />
        <property
            name="changerFullName"
            visibility="no"
            translation="sulu_admin.changer"
        />
        <property
            name="authorFullName"
            visibility="yes"
            translation="sulu_admin.author"
        />
        <property
            name="created"
            type="datetime"
            visibility="no"
            translation="sulu_admin.created"
        />
        <property
            name="changed"
            type="datetime"
            visibility="no"
            translation="sulu_admin.changed"
        />
        <property
            name="authored"
            type="date"
            visibility="no"
            translation="sulu_admin.authored"
        />
    </properties>
</list>
