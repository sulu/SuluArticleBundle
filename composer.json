{
    "name": "sulu/article-bundle",
    "description": "Sulu bundle for managing articles",
    "type": "sulu-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "sulu/sulu": "^2.0",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/force-lowest": "^3.4 || ^4.0",
        "symfony/http-foundation": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0",
        "symfony-cmf/slugifier-api": "^1.0 || ^2.0",
        "ongr/elasticsearch-bundle": "^5.2",
        "ongr/elasticsearch-dsl": "~5.0 || ~6.0",
        "elasticsearch/elasticsearch": "~5.0 || ~6.0",
        "jackalope/jackalope": "^1.3.3",
        "jms/serializer": "^3.0",
        "jms/serializer-bundle": "^3.0"
    },
    "require-dev": {
        "massive/build-bundle": "~0.3.0",
        "zendframework/zend-stdlib": "~2.3",
        "zendframework/zendsearch": "@dev",
        "symfony/browser-kit": "^3.4 || ^4.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/stopwatch": "^3.4 || ^4.0",
        "jackalope/jackalope-doctrine-dbal": "^1.3.4",
        "jackalope/jackalope-jackrabbit": "^1.3",
        "phpunit/phpunit": "^6.5",
        "sulu/automation-bundle": "dev-develop",
        "php-task/task-bundle": "2.0.x-dev",
        "doctrine/data-fixtures": "^1.0",
        "phpstan/phpstan": "^0.11"
    },
    "conflict": {
        "jackalope/jackalope": "< 1.3.4",
        "jackalope/jackalope-jackrabbit": "< 1.3.0",
        "jackalope/jackalope-doctrine-dbal": "< 1.3.2"
    },
    "suggest": {
        "sulu/automation-bundle": "Allows to outsource long-running route update processes."
    },
    "keywords": [
        "articles"
    ],
    "authors": [
        {
            "name": "Sulu Article",
            "homepage": "https://github.com/sulu/SuluArticleBundle/contributors"
        }
    ],
    "homepage": "https://github.com/sulu/SuluArticleBundle",
    "autoload": {
        "psr-4": {
            "Sulu\\Bundle\\ArticleBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-develop": "2.0-dev"
        }
    }
}
